<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>
 	<package name="data" namespace="/" extends="struts-default">
 		<action name="tableExport" class="data.action.ManageDataAction" method="tableExport">
 			
		</action>
		
		<action name="dbExport" class="data.action.ManageDataAction" method="dbExport">
			<result name="error">/DataSave.jsp</result>
		</action>
		
		<action name="dbImport" class="data.action.ManageDataAction" method="dbImport">
			<result name="success">/DataLogin.jsp?result=success</result>
			<result name="error">/DataLogin.jsp?result=error</result>
		</action>
		
		<action name="staffImport" class="data.action.ManageDataAction" method="staffImport">
			<result name="success">/StaffImport.jsp?result=success</result>
			<result name="error">/StaffImport.jsp?result=error</result>
			<result name="dataError">/StaffImport.jsp?result=dataError</result>
			<result name="dataSame">/StaffImport.jsp?result=dataSame</result>
		</action>
		
		<action name="proLineImport" class="data.action.ManageDataAction" method="proLineImport">
			<result name="success">/ProLineImport.jsp?result=success</result>
			<result name="error">/ProLineImport.jsp?result=error</result>
			<result name="dataError">/ProLineImport.jsp?result=dataError</result>
			<result name="dataSame">/ProLineImport.jsp?result=dataSame</result>
		</action>
		
		<action name="proImport" class="data.action.ManageDataAction" method="proImport">
			<result name="success">/ProImport.jsp?result=success</result>
			<result name="error">/ProImport.jsp?result=error</result>
			<result name="dataError">/ProImport.jsp?result=dataError</result>
			<result name="noproLine">/ProImport.jsp?result=noproLine</result>
		</action>
	</package>
</struts>