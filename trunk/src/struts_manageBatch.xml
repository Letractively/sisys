<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="batch" namespace="/" extends="struts-default">
		<interceptors>
			<interceptor name="checkLogin" class="data.interceptor.CheckLoginInterceptor"/>
				<interceptor-stack name="teamwareStack">
					 <interceptor-ref name="checkLogin"/>
	                 <interceptor-ref name="defaultStack"/>
	            </interceptor-stack>
	    </interceptors>	
		<default-interceptor-ref name="teamwareStack"/>
		
		
		<!--定义全局Result-->  
        <global-results>  
            <result name="login">/login.jsp</result>  
        </global-results>
		
		<action name="preAddBatch" class="data.action.ManageBatchAction" method="preAddBatch">
			
			<result name="success">
        		<param name="inputName">inputStream</param>
   			 </result>
		</action>
		
		<action name="addBatch" class="data.action.ManageBatchAction" method="addBatch">
			<result name="false">/BatchAdd.jsp?result=false</result>
			<result name="success">/BatchAdd.jsp?result=success</result>
		</action>
		
		<action name="modifyOutDue" class="data.action.ManageBatchAction" method="modifyOutDue">
			<result name="empty">/OutOfDue.jsp?result=empty</result>
			<result name="none">/OutOfDue.jsp?result=none</result>
			<result name="success">/OutOfDue.jsp?result=success</result>
			<result name="false">/OutOfDue.jsp?result=false</result>
		</action>
		
 
	</package>

</struts> 